{
    "printWidth": 80,
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "semi": false,
    "proseWrap": "preserve",
    "arrowParens": "avoid",
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "overrides": [
        {
          "files": "*.code-workspace",
          "options": {
            "trailingComma": "none"
          }
        }
    ]
}
